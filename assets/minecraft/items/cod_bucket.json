{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:component",
    "component": "minecraft:custom_name",
    "cases": [
      {
        "when": [
          "Angler",
          "iregex:.*Angler.*"
        ],
        "model": {
          "type": "minecraft:model",
          "model": "item/cod_bucket_angler"
        }
      },
      {
        "when": [
          "Sunfish",
          "iregex:.*Sunfish.*"
        ],
        "model": {
          "type": "minecraft:model",
          "model": "item/cod_bucket_sunfish"
        }
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "item/cod_bucket"
    }
  }
}