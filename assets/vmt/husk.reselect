let
    random :=
        if rand(husk, 1, 2) == 1 then
            default
        else
            "minecraft:optifine/mob/zombie/husk2.png"
        end
in
    if husk.has_name then
        if husk.name == "1" then
            default
        elseif husk.name == "2" then
            "minecraft:optifine/mob/zombie/husk2.png"
        else
            random
        end
    else
        random
    end
end