let
    random :=
        if rand(zombie, 1, 2) == 1 then
            default
        else
            "minecraft:optifine/mob/zombie/zombie2.png"
        end
in
    if zombie.has_name then
        if zombie.name == "1" then
            default
        elseif zombie.name == "2" then
            "minecraft:optifine/mob/zombie/zombie2.png"
        else
            random
        end
    else
        random
    end
end