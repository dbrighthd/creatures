let
    random :=
        if rand(sheep, 1, 2) == 1 then
            default
        else
            "minecraft:optifine/mob/sheep/sheep2.png"
        end
in
    if sheep.has_name then
        if sheep.name == "1" then
            default
        elseif sheep.name == "2" then
            "minecraft:optifine/mob/sheep/sheep2.png"
        else
            random
        end
    else
        random
    end
end